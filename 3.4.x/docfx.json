{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "user-documentation/**.md",
          "user-documentation/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "integrations/**.md",
          "integrations/toc.yml"
        ]
      },
      {
        "files": [
          "api-reference/**.md",
          "api-reference/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "logo.svg",
          "favicon.ico",
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Sitecore Content Hub ウェブサイト",
      "_gitContribute": {
        "repo": "https://github.com/SitecoreJapan/ContentHub.docs",
        "branch": "main",
        "apiSpecFolder": "docs-ref-overwrite"
      },
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "images/favico.ico"
    },
    "dest": "../docs/content/3.4.x",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "sitecore"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}